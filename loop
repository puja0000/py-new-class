for i in range (1,20,5):
    p=int(input("enter first number"))
    u=float(input("enter second number"))
    operator=input("enter operator")
    if operator=='+':
     print(p+u)
    elif operator=='-':
        print(p-u)
    elif operator=='*':
        print (p*u)
    elif operator=='/':
        print(p/u)
    else:
        print ("math error")
